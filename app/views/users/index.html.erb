<% provide(:title, "User") %>

<h1>ユーザー一覧</h1>
<div class="col-md-10 col-md-offset-1">
  <%= will_paginate %>
  <table class="table table-condensed table-hover" id="table-uers">
  
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, edit_user_path(user) %></td>
      </tr>
    <% end %>
  </table>
  <%= will_paginate %>
</div>
